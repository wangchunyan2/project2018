<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.myplate.dao.TsPersonInfoMapper" >
  <resultMap id="BaseResultMap" type="com.myplate.pojo.TsPersonInfo" >
    <result column="ID" property="id" jdbcType="INTEGER" />
    <result column="user_name" property="userName" jdbcType="VARCHAR" />
    <result column="user_age" property="userAge" jdbcType="VARCHAR" />
    <result column="user_height" property="userHeight" jdbcType="VARCHAR" />
    <result column="user_weight" property="userWeight" jdbcType="VARCHAR" />
    <result column="create_date" property="createDate" jdbcType="VARCHAR" />
    <result column="create_by" property="createBy" jdbcType="VARCHAR" />
    <result column="user_sex" property="usersex" jdbcType="VARCHAR" />
  </resultMap>

  <insert id="save" parameterType="com.myplate.pojo.TsPersonInfo">
    insert into TS_COMMON_USER(user_name,user_pwd,user_type)
    values(#{userName},#{pwd},#{userType})
  </insert>

</mapper>